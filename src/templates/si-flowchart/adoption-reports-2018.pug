      h1 Restriction on the Preparation of Adoption Reports (Amendment) Regulations 2018
      ul
        li Subject to the #[+link('#', 'draft affirmative procedure')]
        li #[+link('#', 'Read the related information')]

  .section.section--wide
    .container
      h2 Completed steps
      .section--major
        ul.step-container
          li
            ul.step-grid
              li.step-0.s-highlight
                ul
                  li Instrument created
                  li #[+link('#', 'Find out more about this step')]
            ul.step-grid
              li.step-1-1.s-highlight.b-highlight.a-highlight
                ul
                  li House of Commons
                  li Laid before the House of Commons
                  li #[+link('#', 'Find out more about this step')] 
              li.step-1-2.s-highlight.b-highlight.a-highlight
                ul
                  li House of Lords
                  li Laid before the House of Lords
                  li #[+link('#', 'Find out more about this step')] 
            ul.step-grid
              li.step-2-1.s-highlight.b-highlight.a-highlight
                ul
                  li House of Commons
                  li Considered for English votes for English laws (EVEL) certification
                  li #[+link('#', 'Find out more about this step')]
              li.step-2-2.s-highlight.b-highlight.a-highlight
                ul
                  li House of Commons
                  li Motion to approve the instrument tabled by the Government
                  li #[+link('#', 'Find out more about this step')]
              li.step-2-3.g-highlight
                ul
                  li House of Commons
                  li Considered by the Select Committee on Statutory Instruments (SCSI)
              li.step-2-4.s-highlight.b-highlight
